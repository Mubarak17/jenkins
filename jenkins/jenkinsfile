pipeline {
      agent any
      stages {
            stage('Init') {
                  steps {
                         "Hi, this is Mubarak"
                        bat "We are Starting the Testing"
                  }
            }
            stage('Build') {
                  steps {
                        echo "Building Sample Maven Project"
                  }
            }
            stage('Deploy') {
                  steps {
                        bat "Deploying in Staging Area"
                  }
            }
           
      }
}